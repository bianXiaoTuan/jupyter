   "source": [
    "from sklearn.neural_network import MLPClassifier\n",
    "\n",
    "X = [[0., 0.], [1., 1.]]\n",
    "y = [0, 1]\n",
    "\n",
    "clf = MLPClassifier(solver='lbfgs', alpha=1e-5, hidden_layer_sizes=(5, 5, 2), random_state=1)\n",
    "clf.fit(X, y)\n",
    "\n",
    "print clf.predict([[2., 2.], [-1., -2.]])\n",
    "\n",
    "# 参数矩阵\n",
    "[coef.shape for coef in clf.coefs_]"
   ]
